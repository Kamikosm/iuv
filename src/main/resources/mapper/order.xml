<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iuv.dao.OrderDao">
	<insert id="insertOrder" parameterType="com.iuv.pojo.order.Order">
		insert into userorder(id,mv_name,user_id,price,order_time,seat_id,hall,scene_time,movie_img) values(
		null,#{mvName},#{userId},#{price},#{orderTime},#{seatId},#{hall},#{sceneTime},#{movieImg})
	</insert>
</mapper>